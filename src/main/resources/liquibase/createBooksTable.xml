<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    
    <changeSet id="4" author="ereut">
        <createTable tableName="books">
            <column name="id" type="bigint">
                <constraints unique="true" primaryKey="true" nullable="false"/>
            </column>
            <column name="title" type="varchar(30)"/>
        </createTable>
    </changeSet>

    <changeSet id="5" author="ereut">
        <createSequence sequenceName="booksSequence"/>
    </changeSet>
    
    <changeSet id="6" author="ereut">
        <createTable tableName="bookssequence">
            <column name="next_val" type="bigint"></column>
        </createTable>
    </changeSet>

    <changeSet id="7" author="ereut">
        <sql>INSERT into bookssequence (next_val) VALUES (1)</sql>
    </changeSet>

</databaseChangeLog>